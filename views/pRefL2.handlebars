          </div>
          <!-- /.box -->
        </div>

        {{#if getPayTrue}}
        <div class="col-md-3">
          <div class="box box-primary box-solid" style="margin-top: 15px;">
            <!-- /.box-header -->
            <div class="box-body">
              <button type="button" class="btn btn-group-justified btn-info" data-toggle="modal" data-target="#finCodeModal">Get Your Pay!</button>
            </div>
            <!-- /.box-body -->
