        {{/if}}
        {{/if}}

        {{#if passEqual16}}
        <div class="col-md-3">
          <div class="box box-primary box-solid">
            <div class="box-header with-border">
              <h3 class="box-title">Passive Referral</h3>

              <div class="box-tools pull-right">
              </div>
              <!-- /.box-tools -->
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              All Passed Level 1.<span class="fa fa-check"></span>
            </div>
