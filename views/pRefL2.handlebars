<!--Fincode Modal -->
<div class="modal fade in" id="finCodeModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <form action="/travel-bank-get-pay" method="Post" onsubmit="getPayBtnPassv.disabled = true; return true;">
      <div class="modal-header">
        <h4 class="modal-title">Select The Payment Level</h4>
      </div>
      <div class="modal-body">
        <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-key"></i></span>
          <input type="password" class="form-control" name="finCode" placeholder="Travel Bank Code" required autofocus>
        </div>
        <br />
        <div class="row">
          <div class="col-lg-6">
            <div class="input-group">
                  <span class="input-group-addon">
                    <input type="checkbox" name="level1" value="{{Withdrawable1}}">
                  </span>
              <input type="text" class="form-control" value="Level 1 || {{Withdrawable1}}" disabled>
            </div>
            <!-- /input-group -->
          </div>
          <!-- /.col-lg-6 -->
          <div class="col-lg-6">
            <div class="input-group">
                  <span class="input-group-addon">
                    <input type="checkbox" name="level2" value="{{finishedWithdrawable}}">
                  </span>
              <input type="text" class="form-control" value="Level 2 || {{finishedWithdrawable}}" disabled>
            </div>
            <!-- /input-group -->
          </div>
          <!-- /.col-lg-6 -->
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-primary" name="getPayBtnPassv">Get Payment</button>
      </div>
      </form>
    </div>
  </div>
</div>
<!-- /Fincode Modal -->

