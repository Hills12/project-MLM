        {{/unless}}
        
        {{/if}}

        {{#if passMore8}}
        <div class="col-md-3">
          <div class="box box-success box-solid">
            <div class="box-header">
              <h3 class="box-title">Passive Referral</h3>
            </div>
            <div class="box-body">
              {{NumOfPRefPassL1}} Person(s) Passed Level 1.
            </div>
            <!-- /.box-body -->
            <!-- Loading (remove the following to stop the loading)-->
            <div class="overlay">
              <i class="fa fa-refresh fa-spin"></i>
            </div>
            <!-- end loading -->
          </div>
          <!-- /.box -->
        </div>

        {{#if getPayTrue}}
        <div class="col-md-3">
          <div class="box box-primary box-solid" style="margin-top: 15px;">
            <!-- /.box-header -->
            <div class="box-body">
              <button type="button" class="btn btn-group-justified btn-info" data-toggle="modal" data-target="#finCodeModal">Get Your Pay!</button>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        {{/if}}

        {{#if getPayFalse}}
        <div class="col-md-3">
          <div class="box box-warning box-solid">
            <div class="box-header">
              <h3 class="box-title">Request withdrawal</h3>
            </div>
            <div class="box-body">
              Almost There...
            </div>
            <!-- /.box-body -->
            <!-- Loading (remove the following to stop the loading)-->
