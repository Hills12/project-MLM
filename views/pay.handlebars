<body class="hold-transition skin-blue sidebar-mini layout-boxed">
<!-- Site wrapper -->
<div class="wrapper">

  <!-- =============================================== -->
  {{> bars}}
  <!-- =============================================== -->

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        {{packageType}} Startup Payment
      </h1>
    </section>

    <!-- Main content -->
    <section class="content">
      {{{fincodeErr}}}
      <!-- Default box -->
      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title">Quick Tips!!!</h3>

          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                    title="Collapse">
              <i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
              <i class="fa fa-times"></i></button>
          </div>
        </div>
        <div class="box-body">
          The invoice below is for the payment of the startup capital for the {{packageType}} package as explained in the <a href="#">package guide</a>. Carefully cross-check the invoice and then proceed to make your startup payment. 
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
          <strong>The journey begins with a step...</strong>
        </div>
        <!-- /.box-footer-->
      </div>
      <!-- /.box -->

      <!-- =========================================================== -->

      <div class="row">
        <div class="col-md-3 col-sm-6 col-xs-12">
          <div class="info-box bg-aqua">
            <span class="info-box-icon"><i class="fa fa-play"></i></span>

            <div class="info-box-content">
              <span class="info-box-text">Startup</span>
              <span class="info-box-number">{{startup}}</span>

              <div class="progress">
                <div class="progress-bar" style="width: 100%"></div>
              </div>
                  <span class="progress-description">
                    Startup in a bit...
                  </span>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
        <!-- /.col -->

        <div class="col-md-3 col-sm-6 col-xs-12">
          <div class="info-box bg-aqua">
            <span class="info-box-icon"><i class="fa fa-hashtag"></i></span>

            <div class="info-box-content">
              <span class="info-box-text">total Earned</span>
              <span class="info-box-number">{{totalAmount}}</span>

              <div class="progress">
                <div class="progress-bar" style="width: 100%"></div>
              </div>
                  <span class="progress-description">
                    Zero naira made!
                  </span>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
        <!-- /.col -->
        
        <div class="col-md-3 col-sm-6 col-xs-12">
          <div class="info-box bg-aqua">
            <span class="info-box-icon"><i class="fa fa-plane"></i></span>

            <div class="info-box-content">
              <span class="info-box-text">Travel Bank</span>
              <span class="info-box-number">{{tBank}}</span>

              <div class="progress">
                <div class="progress-bar" style="width: 100%"></div>
              </div>
                  <span class="progress-description">
                    Bank is empty!!!
                  </span>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
        <!-- /.col -->
        <div class="col-md-3 col-sm-6 col-xs-12">
          <div class="info-box bg-aqua">
            <span class="info-box-icon"><i class="fa fa-money"></i></span>

            <div class="info-box-content">
              <span class="info-box-text">Withdrawable</span>
              <span class="info-box-number">{{Withdrawable}}</span>

              <div class="progress">
                <div class="progress-bar" style="width: 100%"></div>
              </div>
                  <span class="progress-description">
                    Yet to sow...
                  </span>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

      <!-- =========================================================== -->

    {{#if refId}}
     <!-- SELECT2 EXAMPLE -->
      <div class="box box-default">
        <div class="box-header with-border">
          <h3 class="box-title">Who Refered You?</h3>

          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
          </div>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <label>Referrer Name</label>
                <input class="form-control select2" disabled="disabled" style="width: 100%;" value="{{refName}}" />
              </div>
              <!-- /.form-group -->
            </div>

            <!-- /.col -->
            <div class="col-md-6">
              <div class="form-group">
                <label>Referrer ID</label>
                <input class="form-control select2" disabled="disabled" style="width: 100%;" value="{{refId}}">
              </div>
              <!-- /.form-group -->
              
            </div>
            <!-- /.col -->
          </div>
          <!-- /.row -->
        </div>
      </div>
      <!-- /.box -->
      {{/if }}

      <!-- =========================================================== -->

      <div class="row">

        <div class="col-md-3">
          <div class="box box-warning box-solid">
            <div class="box-header">
              <h3 class="box-title">Startup Payment</h3>
            </div>
            <div class="box-body">
              Cross-check the invoice
            </div>
            <!-- /.box-body -->
            <!-- Loading (remove the following to stop the loading)-->
            <div class="overlay">
              <i class="fa fa-refresh fa-spin"></i>
            </div>
            <!-- end loading -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->

        <div class="col-md-3">
          <div class="box box-danger box-solid">
            <div class="box-header">
              <h3 class="box-title">Direct Referral</h3>
            </div>
            <div class="box-body">
              No direct referral yet
            </div>
            <!-- /.box-body -->
            <!-- Loading (remove the following to stop the loading)-->
            <div class="overlay">
              <i class="fa fa-refresh fa-spin"></i>
            </div>
            <!-- end loading -->
          </div>
          <!-- /.box -->
        </div>

        <div class="col-md-3">
          <div class="box box-danger box-solid">
            <div class="box-header">
              <h3 class="box-title">Passive Referral</h3>
            </div>
            <div class="box-body">
              No Passive referral yet
            </div>
            <!-- /.box-body -->
            <!-- Loading (remove the following to stop the loading)-->
            <div class="overlay">
              <i class="fa fa-refresh fa-spin"></i>
            </div>
            <!-- end loading -->
          </div>
          <!-- /.box -->
        </div>

        <div class="col-md-3">
          <div class="box box-danger box-solid">
            <div class="box-header">
              <h3 class="box-title">Request Withdrawal</h3>
            </div>
            <div class="box-body">
              Not yet ripe for withdrawal
            </div>
            <!-- /.box-body -->
            <!-- Loading (remove the following to stop the loading)-->
            <div class="overlay">
              <i class="fa fa-refresh fa-spin"></i>
            </div>
            <!-- end loading -->
          </div>
          <!-- /.box -->
        </div>

      </div>
      <!-- /.row -->

      <!-- =========================================================== -->

      <!-- Main content -->
    <section class="invoice">
      <!-- title row -->
      <div class="row">
        <div class="col-xs-12">
          <h2 class="page-header">Travcut MLM. ***INVOICE***
            <small class="pull-right">Date: {{date}}</small>
          </h2>
        </div>
        <!-- /.col -->
      </div>
      <!-- info row -->
      <div class="row invoice-info">
        <div class="col-sm-4 invoice-col">
          From
          <address>
            <strong>{{Name}}</strong><br>
            Phone: {{phoneNo}}<br>
            Email: {{email}}
          </address>
        </div>
        <!-- /.col -->
        <div class="col-sm-4 invoice-col">
          To
          <address>
            <strong>Travcut MLM</strong><br>
            Phone: (555) 539-1037<br>
            Email: john.doe@example.com
          </address>
        </div>
        <!-- /.col -->
        <div class="col-sm-4 invoice-col">
          <br>
          <br>
          <b>Order ID:</b> {{financeID}}<br>
          <br>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

      <!-- Table row -->
      <div class="row">
        <div class="col-xs-12 table-responsive">
          <table class="table table-striped">
            <thead>
            <tr>
              <th>Qty</th>
              <th>Product</th>
              <th>Description</th>
              <th>Amount</th>
            </tr>
            </thead>
            <tbody>
            <tr>
              <td>1</td>
              <td>Startup Capital</td>
              <td>Payment for the capital to begin the run through the {{packageType}} Travel Package</td>
              <td>NGN{{packageAmount}}</td>
            </tr>
            </tbody>
          </table>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

      <div class="row">
        <!-- accepted payments column -->
        <div class="col-xs-6">
          <p class="lead">Payment Methods:</p>
          <img src="../../dist/img/credit/visa.png" alt="Visa">
          <img src="../../dist/img/credit/mastercard.png" alt="Mastercard">
        </div>
        <!-- /.col -->
        <div class="col-xs-6">
          <p class="lead">Amount Due {{date}}</p>

          <div class="table-responsive">
            <table class="table">
              <tr>
                <th style="width:50%">{{packageType}} Startup:</th>
                <td>NGN{{packageAmount}}</td>
              </tr>
              <tr>
                <th>Bank Charges (10%)</th>
                <td>NGN1,500</td>
              </tr>
              <tr>
                <th>Total:</th>
                <td>NGN16,500</td>
              </tr>
            </table>
          </div>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->

      <!-- this row will not appear when printing -->
      <div id="payo" class="row no-print">
        <div class="col-xs-12">
          <a href="invoice-print.html" target="_blank" class="btn btn-default"><i class="fa fa-print"></i> Print</a>
          
          <button class="btn btn-success pull-right" data-toggle="modal" data-target="#finRegModal" autofocus><i class="fa fa-credit-card"></i> Proceed Payment</button>
          
          {{!-- <a href="/financial-auth" class="btn btn-success pull-right" autofocus><i class="fa fa-credit-card"></i> Proceed Payment
          </a> --}}
          <button type="button" class="btn btn-primary pull-right" style="margin-right: 5px;">
            <i class="fa fa-download"></i> Generate PDF
          </button>
        </div>
      </div>
    </section>
    <!-- /.content -->

    <!-- =========================================================== -->

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <!--Fincode Modal -->
<div class="modal fade in" id="finRegModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <form action="/financial-auth/{{packageType}}" method="Post" onsubmit="finRegBtn.disabled = true; return true;">
      <div class="modal-header">
        <h4 class="modal-title">Travel Bank Financial Registration</h4>
        <p class="text-info"><strong>PS</strong>: The Information inputed here will used on every financial transaction on this platform.</p>
      </div>
      <div class="modal-body">
        <label>Account Name:</label> <span class="text-info">The bank account name you wish to recieve cash on</span>
        <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-user"></i></span>
          <input type="text" class="form-control" name="accName" required autofocus />
        </div>
        <br />
        <label>Account Number:</label> <span class="text-info">The bank account number you wish to recieve cash on</span>
        <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-credit-card"></i></span>
          <input type="text" class="form-control" name="accNum" required />
        </div>
        <br />
        <label>BVN:</label> <span class="text-info">Your unique Bank Verification Number needed to verify your account ownership</span>
        <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-bank"></i></span>
          <input type="text" class="form-control" name="bvn" required />
        </div>
        <br />
          <label>Bank Name</label>
          <select class="form-control select2 select2-hidden-accessible" name="bankName" style="width: 100%;" required>
            <option value="Access Bank">Access Bank</option>
            <option value="Citibank">Citibank</option>
            <option value="Diamond Bank">Diamond Bank</option>
            <option value="Dynamic Standard Bank">Dynamic Standard Bank</option>
            <option value="Development Financial Pruben Bank">Development Financial Pruben Bank</option>
            <option value="Ecobank Nigeria">Ecobank Nigeria</option>
            <option value="Fidelity Bank Nigeria">Fidelity Bank Nigeria</option>
            <option value="First Bank Nigeria (FBN)">First Bank Nigeria (FBN)</option>
            <option value="First City Monument Bank (FCMB)">First City Monument Bank (FCMB)</option>
            <option value="Guarantee Trust Bank (GTB)">Guarantee Trust Bank (GTB)</option>
            <option value="Heritage Bank plc (HB)">Heritage Bank plc (HB)</option>
            <option value="Keystone Bank Limited">Keystone Bank Limited</option>
            <option value="Providus Bank plc">Providus Bank plc</option>
            <option value="Skye Bank">Skye Bank</option>
            <option value="Stanbic IBTC Bank Nigeria Limited">Stanbic IBTC Bank Nigeria Limited</option>
            <option value="Standard Chartered Bank">Standard Chartered Bank</option>
            <option value="Sterling Bank">Sterling Bank</option>
            <option value="Suntrust Bank Nigeria Limited">Suntrust Bank Nigeria Limited</option>
            <option value="Union Bank of Nigeria">Union Bank of Nigeria</option>
            <option value="United Bank for Africa (UBA)">United Bank for Africa (UBA)</option>
            <option value="Unity Bank plc">Unity Bank plc</option>
            <option value="Wema Bank">Wema Bank</option>
            <option value="Zenith Bank">Zenith Bank</option>
          </select>
        <br />
        <label>Travel Bank Code:</label> <span class="text-info">This code will be required for you to make any financial transaction</span>
        <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-key"></i></span>
          <input type="password" class="form-control" name="finCode" required />
        </div>
        <br />
        <label>Re-type Travel Bank Code:</label>
        <div class="input-group">
          <span class="input-group-addon"><i class="fa fa-key"></i></span>
          <input type="password" class="form-control" name="finCode2" required />
        </div>        
      </div>
      <div class="modal-footer">
        <button type="submit" class="btn btn-primary btn-group-justified" name="finRegBtn">Proceed</button>
      </div>
      </form>
    </div>
  </div>
</div>
<!-- /Fincode Modal -->

