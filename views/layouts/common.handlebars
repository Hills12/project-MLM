<script src="/js/faker.min.js"></script>
<script>
  $(document).ready(function(){
    var first = faker.name.firstName(),
        last = faker.name.lastName(),
        userFirst = "{{username}}"

    $('#firstname').attr("value", userFirst);
    $('#lastname').attr("value", last);
    $('#address').attr("value", faker.address.streetAddress());
    $('#accName').attr("value", userFirst+" "+last);
    $('#username').attr("value", first);
    $('#email').attr("value", first + "@gmail.com");
  });
</script>



